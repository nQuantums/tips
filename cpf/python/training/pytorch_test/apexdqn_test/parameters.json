{
    "db": {
        "connection_string": "dbname=apexdqn user=postgres"
    },
    "env": {
        "state_shape": [10, 84, 84],
        "action_dim": 4,
        "name": "Breakout-v0"
    },

    "actor": {
        "num_actors": 4,
        "T": 100000000,
        "num_steps": 3,
        "epsilon": 0.4,
        "alpha": 7,
        "gamma": 0.99,
        "n_step_transition_batch_size": 5,
        "Q_network_sync_freq": 100,
        "wait_shared_memory_clear": true
    },

    "learner": {
        "q_target_sync_freq": 50,
        "min_replay_mem_size": 1000,
        "replay_sample_size": 64,
        "load_saved_state": "Breakout-v0_RMSProp.pt",
        "model": "dqn",
        "optimizer": "torch.optim.RMSprop({}, lr=0.00025 / 4, weight_decay=0.95, eps=1.5e-7)"
    },

    "replay_memory": {
        "soft_capacity": 15000,
        "priority_exponent": 0.6,
        "importance_sampling_exponent": 0.4
    }
}